<?xml version="1.0" encoding="utf-8"?><!-- custom_progress_bar_progress.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background track -->
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/grayScale08" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <!-- Progress track -->
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/primaryMint" />
                <corners android:radius="20dp" />
            </shape>
        </clip>
    </item>
</layer-list>